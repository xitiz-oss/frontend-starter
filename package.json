    "js-cookie": "^3.0.5",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-auth-kit": "^4.0.2-alpha.11",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
